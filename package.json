    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.548.0",
    "morgan": "^1.10.0",
    "mqtt": "^5.3.4",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "vite": "^7.1.12",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.0.0"
  },
  "main": "server/index.ts",
  "name": "filaprint",
  "description": "Professional Bambu Labs Printer Management System",
  "repository": {
    "url": "https://github.com/Marcus-Murray/filaprint.git",
    "type": "git"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "axios": "^1.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "react-router-dom": "^7.9.4",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "author": "Marcus Murray",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx --config .eslintrc.json",
    "lint:fix": "eslint . --ext .ts,.tsx --config .eslintrc.json --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "test": "jest --config jest.config.js",
    "test:unit": "jest --config jest.config.js --testPathPattern=unit",
    "test:integration": "jest --config jest.config.js --testPathPattern=integration",
    "test:coverage": "jest --config jest.config.js --coverage --coverageThreshold.global.lines=80",
    "test:watch": "jest --config jest.config.js --watch",
    "test:ci": "jest --config jest.config.js --ci --coverage --watchAll=false",
    "test:print-completion": "tsx scripts/test-print-completion.ts",
    "security:scan": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "quality:check": "npm run lint && npm run type-check && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "build": "npm run build:server",
    "build:server": "tsc --project tsconfig.json --outDir dist",
    "build:prod": "NODE_ENV=production npm run build",
    "dev": "tsx server/index.ts",
    "dev:client": "vite --config vite.config.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "dev:watch": "tsx watch server/index.ts",
    "start": "node dist/server/index.js",
    "clean": "rm -rf dist build coverage test-reports",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test:ci && npm run security:scan"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "keywords": [
    "bambu-labs",
    "3d-printer",
    "mqtt",
    "filament-management",
    "maker-tools",
    "typescript",
    "nodejs",
    "express",
    "security",
    "compliance"
  ]
}
